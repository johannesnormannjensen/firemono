{
  "name": "@firemono/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint:all": "npx -y nx run-many --all --target=lint",
    "build:demo": "npx -y run-many --all --target=build --projects=tag:app:demo",
    "e2e:ui": "firebase emulators:exec --only=functions,firestore,auth --import=./exports 'npx nx e2e ui -- --project chromium'",
    "e2e-ci:ui": "firebase emulators:exec --only=functions,firestore,auth --import=./exports 'npx nx e2e ui --skip-nx-cache -- --project chromium'",
    "test:ui": "npx nx run-many --all --t test --projects ui-\\* ui --exclude data",
    "test-ci:ui": "npx nx run-many --all --t test --projects ui-\\* ui --exclude data --reporters @matteoh2o1999/github-actions-jest-reporter",
    "test:firebase": "firebase emulators:exec --only=firestore --import=./exports 'npx nx run-many -t test --projects firebase-\\* functions --exclude data'",
    "test-ci:firebase": "firebase emulators:exec --only=firestore --import=./exports 'npx nx run-many -t test --projects firebase-\\* functions --exclude data --reporters @matteoh2o1999/github-actions-jest-reporter'",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:firestore": "firebase deploy --only firestore:rules",
    "storybook:ui": "npx -y nx storybook ui",
    "translate": "npx nx run ui:translate"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~19.2.0",
    "@angular/compiler": "~19.2.0",
    "@angular/core": "~19.2.0",
    "@angular/fire": "^19.1.0",
    "@angular/forms": "~19.2.0",
    "@angular/platform-browser": "~19.2.0",
    "@angular/platform-browser-dynamic": "~19.2.0",
    "@angular/router": "~19.2.0",
    "firebase": "^11.6.0",
    "firebase-admin": "^13.2.0",
    "firebase-functions": "^6.3.2",
    "functions": "^1.0.9",
    "rxjs": "~7.8.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "~1.14.1",
    "@analogjs/vitest-angular": "~1.14.1",
    "@angular-devkit/build-angular": "~19.2.0",
    "@angular-devkit/core": "~19.2.0",
    "@angular-devkit/schematics": "~19.2.0",
    "@angular/cli": "~19.2.0",
    "@angular/compiler-cli": "~19.2.0",
    "@angular/language-service": "~19.2.0",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "20.7.2",
    "@nx/devkit": "20.7.2",
    "@nx/esbuild": "20.8.0",
    "@nx/eslint": "20.7.2",
    "@nx/eslint-plugin": "20.7.2",
    "@nx/js": "20.8.0",
    "@nx/playwright": "20.7.2",
    "@nx/plugin": "^20.7.2",
    "@nx/vite": "20.7.2",
    "@nx/web": "20.7.2",
    "@nx/workspace": "20.7.2",
    "@playwright/test": "^1.36.0",
    "@schematics/angular": "~19.2.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "18.16.9",
    "@typescript-eslint/utils": "^8.19.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "angular-eslint": "^19.2.0",
    "autoprefixer": "^10.4.0",
